{
  "name": "add-prettier",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "mocha *.spec.*"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "mocha": "^5.1.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.2"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "tslint --fix -c ./tslint.json 'src/**/*.ts'",
      "git add"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.10"
  }
}
