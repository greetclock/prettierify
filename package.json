{
  "name": "prettierify",
  "version": "0.1.0-beta.6",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "mocha **/*.spec.js"
  },
  "preferGlobal": true,
  "bin": {
    "prettierify": "./bin/prettierify.js"
  },
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "tslint --fix -c ./tslint.json 'src/**/*.ts'",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/prettier": "^1.12.2",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "mocha": "^5.1.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "minimist": "^1.2.0",
    "npm": "^6.0.1",
    "prettier": "^1.12.1"
  }
}
